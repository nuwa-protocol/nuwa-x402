  export NODE_PATH="/Users/haichao/code/nuwa-x402/node_modules/.pnpm/mcp-handler@1.0.3_@modelcontextprotocol+sdk@1.20.2_next@15.5.2_@opentelemetry+api@1.9.0_react_ccrmkoa74w4n2pfscvxfku4im4/node_modules/mcp-handler/dist/cli/node_modules:/Users/haichao/code/nuwa-x402/node_modules/.pnpm/mcp-handler@1.0.3_@modelcontextprotocol+sdk@1.20.2_next@15.5.2_@opentelemetry+api@1.9.0_react_ccrmkoa74w4n2pfscvxfku4im4/node_modules/mcp-handler/dist/node_modules:/Users/haichao/code/nuwa-x402/node_modules/.pnpm/mcp-handler@1.0.3_@modelcontextprotocol+sdk@1.20.2_next@15.5.2_@opentelemetry+api@1.9.0_react_ccrmkoa74w4n2pfscvxfku4im4/node_modules/mcp-handler/node_modules:/Users/haichao/code/nuwa-x402/node_modules/.pnpm/mcp-handler@1.0.3_@modelcontextprotocol+sdk@1.20.2_next@15.5.2_@opentelemetry+api@1.9.0_react_ccrmkoa74w4n2pfscvxfku4im4/node_modules:/Users/haichao/code/nuwa-x402/node_modules/.pnpm/node_modules:$NODE_PATH"
